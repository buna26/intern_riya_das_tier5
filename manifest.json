{
    "name": "Riya Das",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "This is a extension building task",
    "icons": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    },
    "action": {
      "defaut_popup":"popup.html"
    },
    "permissions": [
      "tabs",
        "activeTab",
        "alarms",
        "scripting", 
        "storage",
        "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["https://*.stackoverflow.com/*"],
    "content_scripts": [
      {
        "css": ["style.css"],
        "js": ["content.js"],
        "matches": ["https://*.stackoverflow.com/*"],
        "run_at": "document_end"
      }
    ]
  }